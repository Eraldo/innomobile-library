  "scripts": {
    "lib:build": "npm run core && npm run lib:postbuild",
    "lib:postbuild": "ts-node -P scripts/tsconfig.json scripts/tasks/build",
    "lib:pub": "ts-node -P scripts/tsconfig.json scripts/tasks/publish",
    "all": "npm run core && npm run fireuser && npm run firebase && npm run attribution && npm run iap && npm run ads && npm run mapbox && npm run ionic-image-loader && npm run notifications && npm run countdown-timer",
    "ads": "ng run @innomobile/ads:build:production",
    "attribution": "ng run @innomobile/attribution:build:production",
    "core": "ng run @innomobile/core:build:production",
    "countdown-timer": "ng run @innomobile/countdown-timer:build:production",
    "firebase-native": "ng run @innomobile/firebase-native:build:production",
    "fireuser": "ng run @innomobile/fireuser:build:production",
    "iap": "ng run @innomobile/iap:build:production",
    "ionic-image-loader": "ng run @innomobile/ionic-image-loader:build:production",
    "mapbox": "ng run @innomobile/mapbox:build:production",
    "notifications": "ng run @innomobile/notifications:build:production",
    "n2": "ng build --prod @innomobile/notifications && cd dest/@innomobile/notifications && npm pack",
    "ng": "ng",
    "start": "ionic serve",
    "build": "ionic build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ios-release": "ionic cordova build ios --prod --release",
    "ios-debug": "ionic cordova run ios",
    "android-release": "ionic cordova build android --prod --release --buildConfig=build.json",
    "android-debug": "ionic cordova run android",
    "android": "ionic cordova build android --prod --release --device -c --buildConfig=build.json",
    "release": "ionic build --prod --release"
  },
