{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@innomobile/ads": ["./dest/@innomobile/ads"],
      "@innomobile/attribution": ["./dest/@innomobile/attribution"],
      "@innomobile/core": ["./dest/@innomobile/core"],
      "@innomobile/countdown-timer": ["./dest/@innomobile/countdown-timer"],
      "@innomobile/firebase-native": ["./dest/@innomobile/firebase-native"],
      "@innomobile/fireuser": ["./dest/@innomobile/fireuser"],
      "@innomobile/iap": ["./dest/@innomobile/iap"],
      "@innomobile/ionic-image-loader": ["./dest/@innomobile/ionic-image-loader"],
      "@innomobile/mapbox": ["./dest/@innomobile/mapbox"],
      "@innomobile/notifications": ["./dest/@innomobile/notifications"]
    }
  }
}